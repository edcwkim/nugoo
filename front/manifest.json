{
  "name": "PGHACK NUGOO",

  "version": "1.0",
  "description": "This is a custom built extension for allowing users to get the information about certain peaple",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],

  "browser_action": {
      "default_title": "PGHACK",
      "default_icon": "icon16.png"
  },

  "content_scripts" : [
    {
      "matches" : ["http://*/*", "https://*/*"],
	  "css" : ["css/style.css"],
      "js" : ["js/jquery-3.2.1.min.js", "js/type_contentscript.js"],
      "run_at" : "document_start",
      "all_frames" : true
    }
  ],

  "manifest_version": 2
}
